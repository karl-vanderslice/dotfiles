Host *
  PreferredAuthentications publickey,password
  IdentityAgent ~/.ssh/agent.sock
  IdentitiesOnly no
  ServerAliveInterval 30
  ServerAliveCountMax 4
  NumberOfPasswordPrompts 1
  HashKnownHosts yes
  ControlMaster auto
  ControlPersist 10m
  ControlPath ~/.local/state/ssh/control/%C

Host 10.* 192.168.* 172.16.* 172.17.* 172.18.* 172.19.* 172.2[0-9].* 172.3[0-1].*
  PreferredAuthentications publickey,password
  NumberOfPasswordPrompts 1

Host *.vslice.haus
  User karl
  ForwardAgent yes
  Compression yes

Host korriban.vslice.haus
  User kvanderslice
  ForwardAgent yes
  Compression yes
